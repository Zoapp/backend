{
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "env": {
        "browser": false,
        "es6": true,
        "jquery": false,
        "jest": true,
        "node": true
    },
    "globals": {
        "logger": true,
        "jestExpect": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "prettier"
    ],
    "rules": {
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": 1,
        "prefer-const": "error",
        "no-var": "error",
        "prettier/prettier": ["error", {
            "arrowParens": "always",
            "trailingComma": "all"
        }],
        "camelcase": ["error", {"properties": "never", "ignoreDestructuring": true}]
    }
}
